doctype html
html
  head
    meta(charset='UTF-8')
    meta(name='viewport' content='width=device-width, initial-scale=1.0')
    title Produce List
    link(rel='stylesheet' href='/css/style.css')
    link(rel='stylesheet' href='/css/bootstrap-5.3.3-dist/bootstrap-5.3.3-dist/css/bootstrap-grid.css')
  body
    .container.mt-5
      h2 List of users
      table.table.table-bordered.table-striped
        thead
          tr
            th Email
            th Role
            th Branch            
        tbody
          if signups.length
              each signup in signups
                tr
                  td #{signup.email}
                  td #{signup.role}
                  td #{signup.branch}
    script(src='/bootstrap-5.3.3-dist/bootstrap-5.3.3-dist/js/bootstrap.bundle.js')
